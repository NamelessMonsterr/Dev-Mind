{
  "dashboard": {
    "title": "DevMind - System Overview",
    "tags": ["devmind", "overview"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "Search Operations",
        "targets": [
          {
            "expr": "rate(devmind_searches_total[5m])",
            "legendFormat": "Searches/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Operations/sec"},
          {"format": "short"}
        ]
      },
      
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "Search Latency (p95)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(devmind_search_latency_seconds_bucket[5m]))",
            "legendFormat": "p95 latency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(devmind_search_latency_seconds_bucket[5m]))",
            "legendFormat": "p50 latency",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Seconds"},
          {"format": "short"}
        ]
      },
      
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "type": "graph",
        "title": "LLM Response Time",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(devmind_llm_latency_seconds_bucket[5m]))",
            "legendFormat": "p95 {{provider}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Seconds"},
          {"format": "short"}
        ]
      },
      
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "type": "graph",
        "title": "Ingestion Jobs",
        "targets": [
          {
            "expr": "devmind_ingestion_jobs_total",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Jobs"},
          {"format": "short"}
        ]
      },
      
      {
        "id": 5,
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 16},
        "type": "stat",
        "title": "Vector Index Size",
        "targets": [
          {
            "expr": "devmind_vector_index_size{index_name=\"code\"}",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      
      {
        "id": 6,
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 16},
        "type": "stat",
        "title": "Total Chat Messages",
        "targets": [
          {
            "expr": "sum(devmind_chat_messages_total)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      
      {
        "id": 7,
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 16},
        "type": "stat",
        "title": "Embedding Operations",
        "targets": [
          {
            "expr": "sum(devmind_embedding_operations_total)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      }
    ]
  }
}
